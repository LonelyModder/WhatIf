on UnitDestroyed(un) do
begin
	if un = VanHouten then YouLost('VanHouten');
	if un = Kovalenko then 
		begin
			RuOfficerKilled = true;
			Say(VanHouten,'M1cRusUnit-3');
			ChangeMissionObjectives('Out_CD1c');
		end;
	if un = Tatarczuk then 
		begin
			RuMechanicKilled = true;
		end;
	if un in RuUnit then RuUnit := RuUnit diff un;
	if un in RuReinf then RuReinf := RuReinf diff un;
end;